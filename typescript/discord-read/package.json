{
  "name": "@scramjet/discord-read",
  "version": "0.23.0",
  "main": "index.js",
  "author": "pietrzakacper",
  "license": "GPL-3.0",
  "description": "Sequence that reads messages from Discord channel and writes to topic.",
  "keywords": [
    "sample",
    "discord"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/scramjetorg/platform-samples/tree/main/typescript/discord-read"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "postbuild": "cp -r package.json dist/ && (cd dist && npm i --omit=dev)",
    "pack:pub": "DIST_DIR=${DIST_DIR:-.}; si seq pack ./dist -o ${DIST_DIR}/$(basename $(readlink -f ../))-$(basename $(readlink -f .)).tar.gz",
    "pack": "si seq pack ./dist/",
    "clean": "rm -rf ./dist ./*.tar.gz"
  },
  "type": "commonjs",
  "devDependencies": {
    "@scramjet/types": "^0.28.0",
    "@types/node": "15.12.5",
    "scramjet": "^4.36.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@discordjs/builders": "^0.11.0",
    "@discordjs/rest": "^0.2.0-canary.0",
    "discord-api-types": "^0.28.0",
    "discord.js": "^13.10.2",
    "scramjet": "^4.36.7"
  }
}
