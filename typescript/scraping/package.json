{
  "name": "@scramjet/scraping",
  "version": "0.23.0",
  "main": "app.js",
  "author": "pietrzakacper",
  "license": "GPL-3.0",
  "description": "This is a simple and trivial example of scraping web pages.",
  "keywords": [
    "easy",
    "web scraping",
    "Big Data",
    "BI",
    "Data Producer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/scramjetorg/platform-samples/tree/main/typescript/scraping"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "postbuild": "cp -r package.json dist/ && (cd dist && npm i --omit=dev)",
    "pack:pub": "DIST_DIR=${DIST_DIR:-.}; si seq pack ./dist -o ${DIST_DIR}/$(basename $(readlink -f ../))-$(basename $(readlink -f .)).tar.gz",
    "pack": "si seq pack ./dist/",
    "clean": "rm -rf ./dist ./*.tar.gz"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "cheerio": "*",
    "scramjet": "^4.36.8"
  },
  "devDependencies": {
    "@scramjet/types": "^0.28.0",
    "@types/node": "15.12.5",
    "typescript": "^4.7.4"
  }
}
