{
  "name": "@scramjet/mediawiki",
  "version": "0.23.0",
  "main": "index.js",
  "author": "iaforek",
  "license": "GPL-3.0",
  "description": "Sequence that keeps printing mediawiki event stream.",
  "keywords": [
    "easy",
    "MediaWiki",
    "Wikipedia",
    "Big Data",
    "data operator"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/scramjetorg/platform-samples/tree/typescript/mediawiki"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "postbuild": "cp -r package.json dist/ && (cd dist && npm i --omit=dev)",
    "pack:pub": "DIST_DIR=${DIST_DIR:-.}; si seq pack ./dist -o ${DIST_DIR}/$(basename $(readlink -f ../))-$(basename $(readlink -f .)).tar.gz",
    "pack": "si seq pack ./dist/",
    "clean": "rm -rf ./dist ./*.tar.gz"
  },
  "devDependencies": {
    "@scramjet/types": "^0.28.0",
    "@types/node": "15.12.5",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@types/eventsource": "^1.1.9",
    "eventsource": "^1.1.2",
    "scramjet": "^4.36.7"
  }
}
