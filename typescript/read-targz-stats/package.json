{
  "name": "@scramjet/read-targz-stats",
  "version": "0.22.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "postbuild": "cp -r package.json dist/ && (cd dist && npm i --only=production)",
    "clean": "rm -rf ./dist ./*.tar.gz",
    "packTestFiles": "tar -czvf ./testFiles/testTar.tar.gz ./testFiles/someTestFile ./testFiles/someOtherTestFile",
    "pretest": "npm run packTestFiles",
    "test": "ts-node ./test",
    "pack": "si seq pack ./dist/",
    "pack:pub": "DIST_DIR=${DIST_DIR:-.}; si seq pack ./dist -o ${DIST_DIR}/$(basename $(readlink -f ../))-$(basename $(readlink -f .)).tar.gz"
  },
  "engines": {
    "node": ">=10"
  },
  "scramjet": {
    "image": "node"
  },
  "author": "Scramjet <open-source@scramjet.org>",
  "license": "GPL-3.0",
  "devDependencies": {
    "@scramjet/types": "^0.26.1",
    "@types/node": "15.12.5",
    "@types/tar-stream": "^2.2.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.5.5",
    "@scramjet/cli": "^0.26.1"
  },
  "dependencies": {
    "scramjet": "^4.36.6",
    "tar-stream": "^2.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scramjetorg/transform-hub.git"
  }
}
