{
  "name": "@scramjet/crypto-prices",
  "version": "0.13.1",
  "description": "Sequence that keeps printing current crypto prices for a provided pair of currencies every 1s.",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "postbuild": "cp -r package.json dist/ && (cd dist && npm i --only=production)",
    "pack": "si seq pack ./dist/",
    "clean": "rm -rf ./dist ./*.tar.gz",
    "pack:pub": "DIST_DIR=${DIST_DIR:-.}; si seq pack ./dist -o ${DIST_DIR}/$(basename $(readlink -f ../))-$(basename $(readlink -f .)).tar.gz"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@scramjet/types": "^0.26.1",
    "@types/node": "15.12.5",
    "typescript": "^4.3.4",
    "@scramjet/cli": "^0.26.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scramjetorg/transform-hub.git"
  },
  "dependencies": {
    "node-fetch": "^2.6.1",
    "scramjet": "^4.35.20"
  }
}
