{
  "name": "@scramjet/transform-string-stream",
  "version": "0.13.1",
  "description": "Sequence that modifies incoming stream of strings by adding a prefix and a suffix.",
  "main": "index",
  "types": "dist/index",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "postbuild": "cp -r package.json dist/ && (cd dist && npm i --omit=dev)",
    "pack:pub": "DIST_DIR=${DIST_DIR:-.}; si seq pack ./dist -o ${DIST_DIR}/$(basename $(readlink -f ../))-$(basename $(readlink -f .)).tar.gz",
    "pack": "si seq pack ./dist/",
    "clean": "rm -rf ./dist ./*.tar.gz"
  },
  "engines": {
    "node": ">=10"
  },
  "scramjet": {
    "image": "node"
  },
  "author": "Scramjet <open-source@scramjet.org>",
  "license": "GPL-3.0",
  "devDependencies": {
    "@scramjet/types": "^0.26.1",
    "@types/node": "15.12.5",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "scramjet": "^4.36.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scramjetorg/transform-hub.git"
  },
  "assets": [
    "name.txt"
  ]
}
