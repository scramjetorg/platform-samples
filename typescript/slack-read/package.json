{
  "name": "@scramjet/slack-read",
  "version": "0.23.0",
  "main": "index.js",
  "author": "pietrzakacper",
  "license": "GPL-3.0",
  "description": "Sequence that reads messages from Slack and writes to topic.",
  "keywords": [
    "advanced",
    "Slack",
    "massaging",
    "bot",
    "Data Producer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/scramjetorg/platform-samples/tree/main/typescript/slack-read"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "postbuild": "cp -r package.json dist/ && (cd dist && npm i --only=production)",
    "pack:pub": "DIST_DIR=${DIST_DIR:-.}; si seq pack ./dist -o ${DIST_DIR}/$(basename $(readlink -f ../))-$(basename $(readlink -f .)).tar.gz",
    "pack": "si seq pack ./dist/",
    "clean": "rm -rf ./dist ./*.tar.gz"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "ws": "^8.8.1"
  },
  "devDependencies": {
    "@scramjet/types": "^0.28.0",
    "@types/node": "15.12.5",
    "scramjet": "^4.36.7",
    "typescript": "^4.7.4"
  }
}
