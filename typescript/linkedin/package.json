{
  "name": "@scramjet/linkedin",
  "version": "0.23.0",
  "main": "index.js",
  "author": "iaforek",
  "license": "GPL-3.0",
  "description": "This sequence processes CSV input and retrieves additional company data from LinkedIn via RapidAPI",
  "keywords": [
    "advanced",
    "LinkedIn",
    "social media",
    "BI/MarTech",
    "Data Producer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/scramjetorg/platform-samples/tree/main/typescript/linkedin"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "postbuild": "cp -r package.json dist/ && (cd dist && npm i --omit=dev)",
    "pack:pub": "DIST_DIR=${DIST_DIR:-.}; si seq pack ./dist -o ${DIST_DIR}/$(basename $(readlink -f ../))-$(basename $(readlink -f .)).tar.gz",
    "pack": "si seq pack ./dist/",
    "clean": "rm -rf ./dist ./*.tar.gz"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "cheerio": "*",
    "csv-parser": "^3.0.0",
    "scramjet": "^4.36.7"
  },
  "devDependencies": {
    "@scramjet/types": "^0.28.0",
    "@types/node": "15.12.5",
    "typescript": "^4.7.4"
  }
}
