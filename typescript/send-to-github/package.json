{
  "name": "@scramjet/send-to-github",
  "version": "0.22.0",
  "description": "Sequence commiting a JSON object to a file on Github",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "postbuild": "cp -r package.json dist/ && (cd dist && npm i --only=production)",
    "clean": "rm -rf ./dist ./*.tar.gz",
    "pack": "si seq pack ./dist/",
    "pack:pub": "DIST_DIR=${DIST_DIR:-.}; si seq pack ./dist -o ${DIST_DIR}/$(basename $(readlink -f ../))-$(basename $(readlink -f .)).tar.gz"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@scramjet/types": "^0.26.1",
    "@types/node": "15.12.5",
    "typescript": "^4.7.4",
    "@scramjet/cli": "^0.26.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scramjetorg/transform-hub.git"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "scramjet": "^4.36.7"
  }
}
